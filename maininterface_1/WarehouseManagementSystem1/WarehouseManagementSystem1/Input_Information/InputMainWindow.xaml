<Window x:Class="WarehouseManagementSystem1.Input_Information.InputMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WarehouseManagementSystem1.Input_Information"
        mc:Ignorable="d"
        Title="录入信息" Height="400" Width="700">
    <Grid>
        <Label Content="请选择要录入的信息类别" HorizontalAlignment="Left" Margin="210,10,0,0" VerticalAlignment="Top" FontSize="25" FontWeight="Bold"/>
        <Button Content="原料" HorizontalAlignment="Left" Margin="250,70,0,0" VerticalAlignment="Top" Width="200" Height="40" FontSize="20" FontWeight="Bold" Click="Button_Click"/>
        <Button Content="产品" HorizontalAlignment="Left" Margin="250,140,0,0" VerticalAlignment="Top" Width="200" Height="40" FontWeight="Bold" FontSize="20" Click="Button_Click"/>
        <Button Content="商家" HorizontalAlignment="Left" Margin="250,210,0,0" VerticalAlignment="Top" Width="200" Height="40" FontSize="20" FontWeight="Bold" Click="Button_Click"/>
        <Button Content="返回" HorizontalAlignment="Left" Margin="250,280,0,0" VerticalAlignment="Top" Width="200" Height="40" FontWeight="Bold" FontSize="20" Click="Button_Click"/>
    </Grid>
</Window>
