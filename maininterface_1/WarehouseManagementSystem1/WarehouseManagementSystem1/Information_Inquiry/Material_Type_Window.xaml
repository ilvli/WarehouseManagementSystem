<Window x:Class="WarehouseManagementSystem1.Information_Inquiry.Material_Type_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WarehouseManagementSystem1.Information_Inquiry"
        mc:Ignorable="d"
        Title="原料信息查询" Height="400" Width="700">
    <Grid>
        <Label Content="选择要查询的原料类别" HorizontalAlignment="Left" Margin="220,10,0,0" VerticalAlignment="Top" FontSize="25" FontWeight="Bold"/>
        <Button Content="长丝/氨纶" HorizontalAlignment="Left" Margin="250,90,0,0" VerticalAlignment="Top" Width="200" Height="40" FontSize="20" FontWeight="Bold" Click="Button_Click"/>
        <Button Content="纸箱/纸管/塑料袋" HorizontalAlignment="Left" Margin="250,170,0,0" VerticalAlignment="Top" Width="200" RenderTransformOrigin="0.584,2.338" Height="40" FontSize="20" FontWeight="Bold" Click="Button_Click"/>
        <Button Content="返回" HorizontalAlignment="Left" Margin="250,250,0,0" VerticalAlignment="Top" Width="200" RenderTransformOrigin="0.584,2.338" Height="40" FontSize="20" FontWeight="Bold" Click="Button_Click"/>
    </Grid>
</Window>
