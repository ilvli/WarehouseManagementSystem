<Window x:Class="WarehouseManagementSystem1.Information_Inquiry.Merchant_Inquiry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WarehouseManagementSystem1.Information_Inquiry"
        mc:Ignorable="d"
        Title="商家信息查询" Height="450" Width="800">
    <Grid>
        <Label Content="请选择要查询的商家类别" HorizontalAlignment="Left" Margin="203,0,0,0" VerticalAlignment="Top" 
               FontSize="35" FontWeight="Bold"/>
        <Button Content="供货商家" HorizontalAlignment="Left" Margin="300,100,0,0" VerticalAlignment="Top" Width="200" 
                Height="60" FontSize="25" FontWeight="Bold" Click="Button_Click"/>
        <Button Content="收货商家" HorizontalAlignment="Left" Margin="300,185,0,0" VerticalAlignment="Top" Width="200" 
                Height="60" FontSize="25" FontWeight="Bold" Click="Button_Click"/>
        <Button Content="返回" HorizontalAlignment="Left" Margin="300,270,0,0" VerticalAlignment="Top" Width="200" 
                Height="60" FontSize="25" FontWeight="Bold" Click="Button_Click"/>
    </Grid>
</Window>
